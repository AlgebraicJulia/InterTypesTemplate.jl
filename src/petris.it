@schema SchPetri begin
  Species::Ob
  Transition::Ob
  Input::Ob
  Output::Ob
  input_src::Hom(Input, Species)
  input_tgt::Hom(Input, Transition)
  output_src::Hom(Output, Transition)
  output_tgt::Hom(Output, Species)
end

@acset_type PetriNet(SchPetri)

@schema SchPetriWithInfluencer <: SchPetri begin
  Influencer::Ob

  influencer_src::Hom(Influencer, Species)
  influencer_tgt::Hom(Influencer, Transition)
end

@acset_type PetriNetWithInfluencer(SchPetriWithInfluencer)